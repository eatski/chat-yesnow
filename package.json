		"@vitejs/plugin-react": "^4.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"file-system-cache": "^2.1.1",
		"filenamify": "^6.0.0",
		"husky": "^8.0.3",
		"jsdom": "^22.0.0",
		"lint-staged": "^13.2.2",
		"msw": "^1.2.2",
		"msw-trpc": "^1.3.3",
		"node-fetch": "^3.3.1",
		"prisma": "^4.13.0",
		"ts-node": "^10.9.1",
		"vercel": "^29.1.1",
		"vitest": "^0.30.1"
