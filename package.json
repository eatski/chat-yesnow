{
	"name": "chat-yesnow",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"postinstall": "prisma generate",
		"build": "./build.sh",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest",
		"test:prepare": "ts-node -P ./tsconfig.node.json scripts/initTestData.ts",
		"e2e:server": "cross-env NEXT_PUBLIC_DISABLE_RECAPTCHA='$(date)' npm run build && npm run start",
		"e2e:dev": "npx playwright test --ui",
		"e2e:server:dev": "cross-env NEXT_PUBLIC_DISABLE_RECAPTCHA='$(date)' npm run dev",
		"format": "rome check --apply-unsafe src || rome format --write src",
		"prepare": "husky install"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.1.0",
		"@prisma/client": "^4.13.0",
		"@tanstack/react-query": "^4.29.5",
		"@trpc/client": "^10.22.0",
		"@trpc/next": "^10.22.0",
		"@trpc/react-query": "^10.22.0",
		"@trpc/server": "^10.22.0",
		"@types/node": "18.16.2",
		"@types/react": "18.2.0",
		"@types/react-dom": "18.2.1",
		"axios": "^0.26.0",
		"axios-cache-interceptor": "^0.8.4",
		"dayjs": "^1.11.7",
		"eslint": "8.39.0",
		"eslint-config-next": "13.3.1",
		"ml-kmeans": "^6.0.0",
		"next": "^13.4.4",
		"next-auth": "^4.22.1",
		"openai": "^3.2.1",
		"randomstring": "^1.2.3",
		"react": "18.2.0",
		"react-copy-to-clipboard": "^5.1.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.43.9",
		"react-icons": "^4.8.0",
		"react-markdown": "^8.0.7",
		"react-syntax-highlighter": "^15.5.0",
		"rome": "^12.1.2",
		"sanitize.css": "^13.0.0",
		"sass": "^1.62.1",
		"typescript": "^5.1.3",
		"ua-parser-js": "^1.0.35",
		"yaml": "^2.2.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@testing-library/react": "^14.0.0",
		"@types/randomstring": "^1.1.8",
		"@types/react-copy-to-clipboard": "^5.0.4",
		"@types/react-syntax-highlighter": "^15.5.6",
